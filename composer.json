{
    "name": "wucfeng/think-captcha",
    "description": "captcha package for thinkphp5",
    "license": "Apache-2.0",
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "wucfeng\\think-captcha\\": "src/"
        }
    },
    "authors": [
        {
            "name": "wucfeng"
        }
    ],
    "require": {
        "topthink/framework": "5.1.*"
    },
    "require-dev": {
        "topthink/framework": "5.1.*"
    },
    "config": {
        "allow-plugins": {
            "topthink/think-installer": false
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.org"
        }
    }
}
